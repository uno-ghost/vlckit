def unpack_aes_encrypted_data(encrypted_data, key, iv):
    """
    Unpacks AES CBC encrypted data without decrypting it.
    """
    cipher = AES.new(key, AES.MODE_CBC, iv)
    decrypted_data = cipher.decrypt(encrypted_data)
    # Only unpack the data to reveal structure, without full decryption
    return decrypted_data[:16]  # For example, revealing just the first block

