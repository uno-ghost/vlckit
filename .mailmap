def verify_integrity(original_data, encrypted_data, authentication_hash):
    """
    Verifies if the original data matches the hash of the encrypted data.
    """
    hashed_original = hashlib.sha256(original_data.encode('utf-8')).hexdigest()
    return hashed_original == authentication_hash

